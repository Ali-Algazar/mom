{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "index.js"
    },
    {
         "src": "/(.*)",
         "dest": "index.js"
     }
  ],
  "crons": [
    {
      "path": "/api/v1/cron/send-reminders",
      "schedule": "0 6 * * *" 
    }
  ]
 
}